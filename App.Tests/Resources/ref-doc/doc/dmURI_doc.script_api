- desc: URI functions.
  members:
  - desc: URI parsing result
    name: :Result
    parameters: []
    type: function
  - desc: URI parsing result parts
    name: :Parts
    type: struct
  - desc: Parse URI and split in three parts. (scheme, location, path)
    name: :Parse
    parameters:
    - desc: URI to parse
      name: uri
      type: const char*
    - desc: Result
      name: parts
      type: dmURI::Parts
    returns:
    - desc: on success
      type: ''
    type: function
  - desc: Performs URL encoding of the supplied buffer
    name: :Encode
    parameters:
    - desc: string to encode
      name: src
      type: const char*
    - desc: the destination buffer
      name: dst
      type: char*
    - desc: size of the provided out buffer
      name: dst_size
      type: uint32_t
    - desc: number of bytes written
      name: bytes_written[out]
      type: uint32_t
    type: function
  - desc: Decodes an URL encoded buffer
    name: :Decode
    parameters:
    - desc: Input
      name: src
      type: const char*
    - desc: Decoded output
      name: dst
      type: char*
    type: function
  name: dmURI
  type: table
